<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FEC_APP.Views.Responsavel.ListarResponsavel"
             Title="Listar Responsáveis">
    <ContentPage.Content>
        <StackLayout>
            <Frame Margin="10, 10, 15, 10" BackgroundColor="{StaticResource BackgroundColorApp}">
                <Label Text="Responsáveis Cadastrados" FontFamily="{StaticResource RubikBold}"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
            </Frame>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>